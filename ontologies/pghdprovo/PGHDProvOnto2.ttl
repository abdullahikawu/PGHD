@prefix : <https://w3id.org/pghdprovo/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://foaf.w3.org#> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pghdprov: <https://w3id.org/pghdprovo#> .
@prefix prov: <https://www.w3.org/TR/prov-o/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix saref4wear: <http://www.owl-ontologies.com/saref4wear#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

foaf:Person a owl:Class ;
    rdfs:label "Person" ;
    owl:equivalentClass prov:Person .

dc:creator a owl:AnnotationProperty .

dc:title a owl:AnnotationProperty .

saref4wear:User a owl:Class ;
    rdfs:label "User" .

pghdprov:PGHDRequestedBy a owl:ObjectProperty ;
    rdfs:label "p g h d requested by" ;
    rdfs:domain pghdprov:PGHDRequest ;
    rdfs:range pghdprov:Practitioner .

: a owl:Ontology ;
    dc:creator "Abdullahi Abubakar Kawu" ;
    dc:title "PGHD-PROV Ontology" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

prov:ActedonBehalfOf a owl:ObjectProperty ;
    rdfs:label "actedon behalf of" ;
    rdfs:domain pghdprov:PatientRelative ;
    rdfs:range pghdprov:Patient .

prov:IsAStaffOf a owl:ObjectProperty ;
    rdfs:label "is a staff of" ;
    rdfs:domain pghdprov:Practitioner ;
    rdfs:range prov:Organization .

prov:endedAtTime a owl:DatatypeProperty ;
    rdfs:label "ended at time" ;
    rdfs:domain pghdprov:PGHDRequest .

prov:used a owl:ObjectProperty ;
    rdfs:label "used" ;
    rdfs:domain pghdprov:Application .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:label "was associated with" ;
    rdfs:domain pghdprov:PGHDRequest ;
    rdfs:range pghdprov:Patient .

prov:wasAttributedTo a owl:ObjectProperty ;
    rdfs:label "was attributed to" ;
    rdfs:domain pghdprov:PGHD ;
    rdfs:range pghdprov:Patient .

prov:wasAuthorizedBy a owl:ObjectProperty ;
    rdfs:label "was authorized by" ;
    rdfs:domain pghdprov:PGHDRequest ;
    rdfs:range pghdprov:Patient .

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by" .

pghdprov:Application a owl:Class ;
    rdfs:label "Application" .

pghdprov:activityId a owl:DatatypeProperty ;
    rdfs:label "activity id" ;
    rdfs:domain pghdprov:PGHDRequest .

pghdprov:datasource a owl:DatatypeProperty ;
    rdfs:label "datasource" ;
    rdfs:domain pghdprov:PGHD .

pghdprov:dateCollected a owl:DatatypeProperty ;
    rdfs:label "date collected" ;
    rdfs:domain pghdprov:PGHD .

prov:Activity a owl:Class ;
    rdfs:label "Activity" .

prov:Entity a owl:Class ;
    rdfs:label "Entity" .

prov:startedAtTime a owl:DatatypeProperty ;
    rdfs:label "started at time" ;
    rdfs:domain pghdprov:PGHDRequest .

prov:Agent a owl:Class ;
    rdfs:label "Agent" .

pghdprov:PGHD a owl:Class ;
    rdfs:label "P G H D" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:datasource ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:dateCollected ],
        prov:Entity .

pghdprov:PatientRelative a owl:Class ;
    rdfs:label "Patient Relative" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:id ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:name ],
        prov:Person .

prov:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:subClassOf prov:Agent ;
    owl:disjointWith prov:Organization .

pghdprov:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    rdfs:domain pghdprov:Patient,
        pghdprov:PatientRelative,
        pghdprov:Practitioner,
        prov:Organization .

pghdprov:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain pghdprov:Patient,
        pghdprov:PatientRelative,
        pghdprov:Practitioner,
        prov:Organization .

prov:Organization a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:id ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:name ],
        prov:Agent ;
    owl:disjointWith prov:Person .

pghdprov:Practitioner a owl:Class ;
    rdfs:label "Practitioner" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:id ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:name ] ;
    owl:disjointWith pghdprov:Patient .

pghdprov:PGHDRequest a owl:Class ;
    rdfs:label "P G H D Request" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:activityId ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty prov:startedAtTime ],
        prov:Activity .

pghdprov:Patient a owl:Class ;
    rdfs:label "Patient" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:id ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty pghdprov:name ] ;
    owl:disjointWith pghdprov:Practitioner .

